<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyTitle>DiscordAPI</AssemblyTitle>
    <Product>DiscordAPI</Product>
    <Copyright></Copyright>
    <AssemblyVersion>1.3.18.0</AssemblyVersion>
    <FileVersion>1.3.18.0</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <AutoGenerateBindingRedirects>False</AutoGenerateBindingRedirects>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.ComponentModel.Composition" />
    <Reference Include="System.Speech" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="Discord.Net.Commands" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="Discord.Net.Core" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="Discord.Net.Interactions" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="Discord.Net.Rest" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="Discord.Net.Webhook" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="Discord.Net.WebSocket" Version="3.18.0-nightly.20250921.631" />
    <PackageReference Include="NAudio" Version="2.2.1" />
  </ItemGroup>
</Project>